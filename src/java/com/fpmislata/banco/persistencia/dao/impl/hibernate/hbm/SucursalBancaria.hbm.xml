<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.SucursalBancaria" table="sucursalBancaria" >
        <id column="idSucursalBancaria" name="idSucursalBancaria" type="integer">
            <generator class="identity"/>
        </id>
        <property name="nombreSucursalBancaria" column="nombreSucursalBancaria" />
        <property name="direccionSucursalBancaria" column="direccionSucursalBancaria" />
        <property name="idEntidadBancaria" column="idEntidadBancaria" />
    </class>
</hibernate-mapping>
