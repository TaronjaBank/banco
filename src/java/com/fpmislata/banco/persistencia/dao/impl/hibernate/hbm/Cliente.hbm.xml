<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.Cliente" table="cliente" >
        <id column="idCliente" name="idCliente" type="integer">
            <generator class="identity"/>
        </id>
        <property name="dniCliente" column="dniCliente" />
        <property name="nombreCliente" column="nombreCliente" />
        <property name="apellido1Cliente" column="apellido1Cliente" />
        <property name="apellido2Cliente" column="apellido2Cliente" />
    </class>
</hibernate-mapping>
