<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.EntidadBancaria" table="entidadBancaria" >
        <id column="idEntidadBancaria" name="idEntidadBancaria" type="integer">
            <generator class="identity"/>
        </id>
        <property name="codigoEntidadBancaria" column="codigoEntidadBancaria" />
        <property name="nombreEntidadBancaria" column="nombreEntidadBancaria" />
        <property name="fechaCreacionEntidadBancaria" column="fechaCreacionEntidadBancaria" />
    </class>
</hibernate-mapping>
