<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.CuentaBancaria" table="cuentaBancaria" >
        <id column="idCuentaBancaria" name="idCuentaBancaria" type="integer">
            <generator class="identity"/>
        </id>
        <property name="numeroCuentaBancaria" column="numeroCuentaBancaria" />
        <property name="idEntidadBancaria" column="idEntidadBancaria" />
        <property name="idCliente" column="idCliente" />
    </class>
</hibernate-mapping>
